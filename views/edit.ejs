  <section class="home-content">
    <div class="margin-left: 20px;">
        <div class="col position-absolute add-pattern add-one mb-5" style="padding-bottom: -300px; padding-top: -100px;">
          <form action="/dashboard/modification/<%=enfant._id%>" method="post" enctype="multipart/form-data">
            <div class="row">
              <div class="col-xl-4 col-lg-1 flex-nowrap">
                <div class="card shadow flex-nowrap" style=" border: 0;padding: 20px 0; padding-left: 10px; line-height: 1.3;padding-top: 10px;margin-top: 10px; border-radius: 20px;">
                 <center style="margin-top:-15px;padding-top: 10px;">
                    <!-- enfant -->
                <p class="big-font" style="font-size: 28px;">Renseignement de l'enfant</p>
                <!-- nom famille -->
                <div class="form-group w-75">
                  <label for="nom-famille"></label>
                  <input type="text" class="form-control shadow shadow" id="nom-famille" placeholder="Nom" name="e_nom" value="<%=enfant.nom%>">
                </div>
                <!-- prenom -->
                <div class="form-group w-75">
                  <label for="prenom"></label>
                  <input type="text" class="form-control shadow shadow" id="prénom" placeholder="Prénom" name="e_prenom" value="<%=enfant.prenom%>">
                </div>
                <!-- date de naissance -->
                <div class="form-group w-75">
                  <label for="date-naissance"></label>
                  <input type="date" class="form-control shadow shadow" data-date-format="DD-MM-YYYY"  name="e_date"  id="date-naissance" placeholder="date de naissance" value="<%=(enfant.date_naissance).toISOString().split('T')[0]%>" >
                </div>
                <!-- lieu de naissance -->
                <div class="form-group w-75">
                  <label for="lieu-naissance"></label>
                  <input type="text" class="form-control shadow shadow" id="lieu-naissance" name="e_lieu" placeholder="Lieu de naissance" value="<%=enfant.lieu_naissance%>">
                </div>
                </center>
                <!-- sexe -->
                <div class="form-group w-75" style="margin-top: 40px; margin-left:50px;">
                  <label for="sex" style="display: block;" class="text-500">Sexe</label>
                  <input type="checkbox" id="sexe" name="e_garcon" <%if(enfant.sexe === 1){%> <%= 'checked'%> <%}%> > Garçon
                  <input type="checkbox" id="sexe" name="e_fille" style="margin-left: 10px;" <%if(enfant.sexe === 0){%> <%= 'checked'%> <%}%>> Fille
                </div>
                <!-- nationnalité -->
                <center>
                <div class="form-group w-75">
                  <label for="nationnalite"></label>
                  <input type="text" name="e_nationnalite" class="form-control shadow shadow" id="nationnalite" placeholder="Nationnalité" value="<%=enfant.nationnalite%>">
                </div>
                <!-- langue maternelle -->
                <div class="form-group w-75">
                  <label for="lang-maternelle"></label>
                  <input type="text" name="e_maternelle" class="form-control shadow shadow" id="lang-maternelle" placeholder="Langue maternelle" value="<%=enfant.langue_maternelle%>">
                </div>
                <!-- langues parlées -->
                <div class="form-group w-75">
                  <label for="lang-parlees"></label>
                  <textarea class="form-control shadow shadow" name="e_parlees" id="lang-parlees" placeholder="Langues parlées"><%=enfant.langues_parlees%></textarea>
                </div>
              </center>
                <div class="form-group w-75" style="margin-top:40px; margin-left: 50px;">
                  <label for="classes" class="text-500">Classe:</label>
                  <br>
                  <select class="form-select" name="classe" id="classe">
                    <%for(let i=1;i<=nbr_classe;i++){%>
                      <option value="<%=i%>" <%if(enfant.classe == i){%>selected<%}%> >
                        <%if(i==1){%>
                          Swans
                        <%}%>
                        <%if(i == 2){%>
                          Little foxes
                        <%}%>
                        <%if(i == 3){%>
                          Foxes
                        <%}%>
                        <%if(i == 4){%>
                          Bears
                        <%}%>
                        <%if(i == 5){%>
                          Seals
                        <%}%>
                        <%if(i == 6){%>
                          Owls
                        <%}%>
                      </option>
                    <%}%>
                  </select>
                </div>
                <div class="form-group w-75" style="margin-top:30px;  margin-left: 50px;">
                  <label for="type" class="text-500">Type enfant:</label>
                  <br>
                  <select class="form-select" name="type_eleve" id="classes">
                    <option value="1" <%if(enfant.type_eleve == 1){%>selected<%}%> >Personnel</option>
                      <option value="2" <%if(enfant.type_eleve == 2){%>selected<%}%> >Internne</option>
                      <option value="3" <%if(enfant.type_eleve == 3){%>selected<%}%>  >Externe</option>
                  </select>
                </div>
                <div class="form-group w-75" style="margin-top: 40px;  margin-left: 50px;">
                  <label for="sm" style="display: block;" class="text-500">Réseaux sociaux</label>
                  <input type="radio" id="sm" name="sm" value="oui" <%if(enfant.sm){%>checked<%}%>> Oui
                  <input type="radio" id="sm" name="sm" value="non" style="margin-left: 15px;" <%if(!enfant.sm){%>checked<%}%>> Non
                </div>
                <div class="form-group w-75" style="margin-top: 40px;  margin-left: 50px;">
                  <label for="ancien" style="display: block;" class="text-500">Enfant ancien</label>
                  <input type="radio" id="ancien" name="ancien_oui" <%if(enfant.type_scolarite == 1){%>checked<%}%>> Oui
                  <input type="radio" id="ancien" name="ancien_non" style="margin-left: 15px;" <%if(enfant.type_scolarite == 2){%>checked<%}%>> Non
                </div>
                </div>
              </div>
              

              <div class="col flex-nowrap" style="margin-top: 40px; width: 100px;">
                <!-- urgence -->
                <div class="row">
                  <div class="card shadow flex-nowrap" style="border: 0;padding: 20px 0; line-height: 1.3; padding-left: 10px; padding-right:10px; border-radius: 20px; margin-top:-30px; margin-left: 20px;">
                    <center>
                    <p class="big-font" style="margin-top: 10px; font-size:28px;">Personne a contacter en cas d'urgence</p>
                  <div class="form-groupe w-75">
                    <label for="nom-urgence"></label>
                    <input type="text" id="nom-urgence" class="form-control shadow" placeholder="Nom" name="nom_urgence" value="<%=urgence.nom%>">
                  </div>
                  <div class="form-groupe w-75" style="margin-top: -7px;">
                    <label for="lien-urgence"></label>
                    <input type="text" id="lien-urgence" class="form-control shadow" placeholder="Lien avec enfant" name="lien_urgence" value="<%=urgence.lien%>">
                  </div>
                  <div class="form-groupe w-75" style="margin-top: -7px;">
                    <label for="tele-urgence"></label>
                    <input type="text" id="tele-urgence" class="form-control shadow" placeholder="N° téléphone" name="tel_urgence" value="<%=urgence.telephone%>">
                  </div>
                </center>
                  </div>
                </div>
                <div class="row">
                  <div class="col">
                    <div class="card shadow flex-nowrap" style="border: 0;padding: 20px 0; padding-left: 10px;border-radius: 20px; margin-top: 20px; margin-left: 5px;">
                      <center>
                        <span style="margin-left: 5px; font-size: 28px;" class="big-font">Kipina Sessions</span>
                      </center>
                      <div style="margin-top: 10px; margin-left: 10px;  margin-left: 50px;" class="text-500">Forfaits</div>
                          <div class="form-group" style="margin-top: 10px;margin-left: 10px; margin-left: 50px;">
                            <label for="complete"></label>
                            <input type="checkbox" id="complete" name="forfait" value="1"  <%if(enfant.type_forfait.includes(1)){%>checked<%}%> ><span style="margin-left: 10px;">Journée complète </span>
                          </div>
                          <div class="form-group" style="margin-top: 10px; margin-left: 10px;  margin-left: 50px;">
                            <label for="AM"></label>
                            <input type="checkbox" id="AM" name="forfait" value="2" <%if(enfant.type_forfait.includes(2)){%>checked<%}%>  ><span style="margin-left: 10px;">AM</span> 
                          </div>
                          <div class="form-group" style="margin-top: 10px;margin-left: 10px;  margin-left: 50px;">
                            <label for="complete"></label>
                            <input type="checkbox" id="PM" name="forfait" value="3" <%if(enfant.type_forfait.includes(3)){%>checked<%}%> ><span style="margin-left: 10px;">PM </span> 
                          </div>
                          <div class="form-group" style="margin-top: 10px;margin-left: 10px;  margin-left: 50px;">
                            <label for="2jrs"></label>
                            <input type="checkbox" id="2jrs" name="forfait" value="4" <%if(enfant.type_forfait.includes(4)){%>checked<%}%> ><span style="margin-left: 10px;">2 jours</span>
                          </div>
                          <div class="form-group" style="margin-top: 10px;margin-left: 10px;  margin-left: 50px;">
                            <label for="3jrs"></label>
                            <input type="checkbox" id="3jrs" name="forfait" value="5" <%if(enfant.type_forfait.includes(5)){%>checked<%}%> ><span style="margin-left: 10px;">3 jours</span>
                          </div>
                          <div style="margin-top: 10px;margin-left: 10px; margin-left: 50px;" class="text-500">Service de gardes</div>
                          <div class="form-group" style="margin-top: 10px; margin-left: 10px;  margin-left: 50px;">
                            <label for="s_am"></label>
                            <input type="checkbox" id="s_am" name="service" value="1" <%if(enfant.gardes_type.includes(1)){%>checked<%}%> ><span style="margin-left: 10px;">AM</span>
                          </div>
                          <div class="form-group" style="margin-top: 10px;margin-left: 10px;  margin-left: 50px;">
                            <label for="s_pm"></label>
                            <input type="checkbox" id="s_pm" name="service" value="2" <%if(enfant.gardes_type.includes(2)){%>checked<%}%> ><span style="margin-left: 10px;">PM</span>
                          </div>
                          <div style="margin-top: 20px;margin-left: 10px; margin-left: 50px;" class="text-500">Mercredi après-midi</div>
                          <div class="form-group" style="margin-top: 10px;margin-left: 10px;  margin-left: 50px;">
                            <label for="m_oui"></label>
                            <input type="checkbox" id="m_oui" name="mercredi" value="1" <%if(enfant.mercredi){%>checked<%}%>> <span style="margin-left: 10px;">Oui</span>
                            </div>
                          <div class="form-group" style="margin-top: 10px;margin-left: 10px; margin-left: 50px;">
                            <label for="m_non"></label>
                            <input type="checkbox" id="m_non" name="mercredi" value="2" <%if(!enfant.mercredi){%>checked<%}%> ><span style="margin-left: 10px;">Non</span>
                          </div>
                          <div style="margin-top: 20px;margin-left: 10px; margin-left: 50px;" class="text-500">Cantine</div>
                          <div class="form-group" style="margin-top: 10px;margin-left: 10px; margin-left: 50px;">
                            <label for="c_4jrs"></label>
                            <input type="checkbox" id="c_4jrs" name="cantine" value="1" <%if(enfant.type_cantine == 1){%>checked<%}%> ><span style="margin-left: 10px;">4 Jours</span>
                            </div>
                          <div class="form-group" style="margin-top: 10px;margin-left: 10px; margin-left: 50px;">
                            <label for="c_5jrs"></label>
                            <input type="checkbox" id="c_5jrs" name="cantine" value="2" <%if(enfant.type_cantine == 2){%>checked<%}%> ><span style="margin-left: 10px;">5 Jours</span>
                          </div>
                          <div class="form-group" style="margin-top: 10px;margin-left: 10px; margin-left: 50px;">
                            <label for="gouter"></label>
                            <input type="checkbox" id="gouter" name="cantine" value="3" <%if(enfant.type_cantine == 3){%>checked<%}%> > <span style="margin-left: 10px;">Goûter</span>
                          </div>
                          <div style="margin-top: 20px;margin-left: 10px; margin-left: 50px;" class="text-500">Transport</div>
                          <div class="form-group" style="margin-top: 10px;margin-left: 10px; margin-left: 50px;">
                            <label for="aller"></label>
                            <input type="checkbox" id="aller" name="transport" value="1" <%if(enfant.type_transport == 1){%>checked<%}%> > <span style="margin-left: 10px;">Aller</span>
                            </div>
                          <div class="form-group" style="margin-top: 10px;margin-left: 10px; margin-left: 50px;">
                            <label for="retour"></label>
                            <input type="checkbox" id="retour" name="transport" value="2" <%if(enfant.type_transport == 2){%>checked<%}%> > <span style="margin-left: 10px;">Retour</span>
                          </div>
                          <div class="form-group" style="margin-top: 10px;margin-left: 10px; margin-left: 50px;">
                            <label for="aller-retour"></label>
                            <input type="checkbox" id="aller-retour" name="transport" value="3" <%if(enfant.type_transport == 3){%>checked<%}%> > <span style="margin-left: 10px;">Aller-retour</span>
                          </div>
                      <!-- <label for="s7"></label>
                      <input type="checkbox" id="s7" name="s7"> Cantine 5 jrs
                    </div> -->
                    </div>
                  </div>
                </div>
              </div>
              <!-- medicaux -->
              <div class="col-xl-4 col-lg-1 flex-nowrap" style="margin-top: 40px;">
                <div class="card shadow flex-nowrap" style="line-height: 1.3;border:0;height: 600px; overflow-y:scroll;padding: 20px 0; padding-left: 10px;border-radius: 20px; margin-top: -25px;">
                  <center>
                  <p class="big-font" style="font-size: 28px;">Renseignement Médicaux</p>
                <div class="form-group w-75" style="margin-left: 10px; margin-top: 20px;">
                  
                  <input type="text" class="form-control shadow" id="text" placeholder="Médecin de l'enfant" name="medcin_nom" value="<%=medical.nom%>" >
                </div>
                <div class="form-group w-75" style="margin-left: 10px; margin-top: 20px;">
                  
                  <input type="text" class="form-control shadow" id="text" placeholder="téléphone Médecin"name="medcin_tel" value="<%=medical.telephone%>" >
                </div>
                <div class="form-group w-75" style="margin-left: 10px; margin-top: 20px;">
                  
                  <input type="text" class="form-control shadow" id="text" placeholder="adresse Médecin" name="adresse_medcine" value="<%=medical.adresse_medcine%>" >
                </div>
              </center>
                <!-- <div class="form-group w-75" style="margin-left: 10px; margin-top: 20px;">
                  
                  <textarea type="text" class="form-control shadow" id="text" placeholder="adresse clinique" name="adresse_clinique"></textarea>
                </div> -->
                <div class="form-group w-75" style="margin-left: 50px; margin-top:20px;">
                  <label id="text" class="text-500" >votre enfant a-t-il des problèmes de santé?</label>
                  <input type="checkbox"  id="text" style="margin-top: 8px;" name="prob_oui" <%if(medical.prob_sante){%>checked<%}%>  > Oui
                  <input type="checkbox"  id="text" name="prob_non" style="margin-left:10px;" <%if(!medical.prob_sante){%>checked<%}%>  > Non
                </div>
                <div class="form-group w-75">
                  <label id="text" style="margin-top: 20px; margin-left: 50px;" class="text-500">Si oui, veuillez-précisez:</label>
                  <textarea type="text" class="form-control shadow" id="text" name="desc_prob_sante" style="margin-left: 50px; margin-top: 8px;"><%=medical.type_prob%></textarea>
                </div>
                <div class="form-group w-75" style="margin-left: 50px; margin-top: 10px;">
                  <label id="text" style="margin-top: 20px; margin-top: 10px;" class="text-500">votre enfant a-t-il eu des maladies ou un des symptômes suivants?</label>
                </div>
                <fieldset>
                  <div class="row">
                    <div class="col-lg-5" style="margin-left: 50px; white-space: nowrap;">
                      <div class="form-group w-75" style="margin-top: 20px;">
                        <input type="checkbox"  id="text" name="s11" <%if(medical.type_maladie.includes(1)){%>checked<%}%> > Allergies
